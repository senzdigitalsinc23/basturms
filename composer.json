{
    "name": "senzdigitals/framework",
    "description": "A custom PHP framework for personal projects",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^3.9",
        "firebase/php-jwt": "^6.11",
        "phpmailer/phpmailer": "^6.10",
        "twilio/sdk": "^8.7",
        "tecnickcom/tcpdf": "^6.10",
        "phpoffice/phpword": "^1.3",
        "phpoffice/phpspreadsheet": "^5.0",
        "stripe/stripe-php": "^17.5",
        "paypal/rest-api-sdk-php": "^1.6"
    },
    "authors": [
        {
            "name": "Douglas Senzu",
            "email": "senzdigitals.inc23@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "App/",
            "App\\Core\\": "Core/",
            "Database\\":"Database/",
            "Services\\": "Services/",
            "Support\\" : "Support/",
            "Repositories\\": "Repositories/"
        },
        "files": [
            "./Core/helpers.php"
        ]       
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "serve": "php -S localhost:8000 -t public"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
